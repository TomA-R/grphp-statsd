{
  "name": "bigcommerce/grphp-statsd",
  "type": "library",
  "description": "grphp statsd interceptor",
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "domnikl/statsd": "~2.0"
  },
  "require-dev": {
    "bigcommerce/grphp": "0.3.8",
    "phpunit/php-code-coverage": "^5.2@dev",
    "phpunit/phpunit": "6.1.*"
  },
  "config": {
    "platform": {
      "php": "7.0.30"
    }
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:bigcommerce/grphp.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
